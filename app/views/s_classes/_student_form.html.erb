<%= simple_form_for [s_class, student], html: { class: "form-control" } do |f| %>
  <%= f.input :name %>
  <%= f.input :kanji_name %>
  <%= f.input :gender, collection: ["Male", "Female", "Other", "Unknown"] %>
  <%= f.input :student_number %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Save Student" %>
<% end %>
